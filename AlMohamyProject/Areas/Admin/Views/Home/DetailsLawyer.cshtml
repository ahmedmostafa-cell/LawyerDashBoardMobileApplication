@model AlMohamyProject.Models.HomePageModel
<div class="container-xxl flex-grow-1 container-p-y">
    <h4 class="py-3 breadcrumb-wrapper mb-4">
        <span class="text-muted fw-light">علوحة التحكم/</span>  تفاصيل المحامي
    </h4>

    <!-- Basic Layout -->
    <div class="row">
        <div class="col-xl">



            <!-- Multi Column with Form Separator -->
            <div class="card mb-4">
                <h5 class="card-header"> المحامي</h5>
                <form enctype="multipart/form-data" method="post" role="form" asp-area="Admin" asp-controller="ConsultingType" asp-action="Save" class="card-body">


                    <div class="row g-3">
                        <div class="col-md-12">
                            <input type="hidden" asp-for="OneUser.Id">
                           
                            <label class="form-label" for="basic-default-fullname">اسم الاكونت</label>
                            <input disabled asp-for="OneUser.UserName" type="text" class="form-control" id="basic-default-fullname" placeholder=" اسم الاكونت" />
                          
                        </div>
                       
                      

                    </div>


                    <div class="row g-3">
                        <div class="col-md-12">
                           
                            <label class="form-label" for="basic-default-fullname"> ايميل المحامي</label>
                            <input disabled asp-for="Email" type="text" class="form-control" id="basic-default-fullname" placeholder="    ايميل المحامي" />

                        </div>



                    </div>


                    <div class="row g-3">
                        <div class="col-md-12">

                            <label class="form-label" for="basic-default-fullname">رقم تليفون المحامي </label>
                            <input disabled asp-for="OneUser.PhoneNumber" type="text" class="form-control" id="basic-default-fullname" placeholder="رقم تليفون المحامي" />

                        </div>



                    </div>



                    <br />
                    <br />
                    <br />


                    <div class="row g-3">
                        <div class="col-md-12">


                            <label class="form-label" for="basic-default-fullname"> صورة المحامي </label>
                            <div class="avatar avatar-xl">
                                <img wid src="@string.Format("/Uploads/{0}",Model.OneUser.Image)" alt="Avatar" class="rounded-circle">
                            </div>
                           

                        </div>



                    </div>

                   


                    <div class="row g-3">
                        <div class="col-md-12">

                            <label class="form-label" for="basic-default-fullname"> قيمة الاستشارة</label>
                            <input disabled asp-for="OneUser.Cost" type="text" class="form-control" id="basic-default-fullname" placeholder=" قيمة الاستشارة" />

                        </div>



                    </div>



                    <div class="row g-3">
                        <div class="col-md-12">

                            <label class="form-label" for="basic-default-fullname">  عدد ايام العمل </label>
                            <input disabled asp-for="OneUser.DaysOfWork" type="text" class="form-control" id="basic-default-fullname" placeholder="عدد ايام العمل" />

                        </div>



                    </div>

                    <br />
                    <br />
                    <br />
                  
                    @if(Model.OneUser.DocumentA!=null) 
                    {
                        <div class="row g-3">
                            <div class="col-md-12">


                                <a href="@string.Format("/Uploads/{0}",Model.OneUser.DocumentA)" class="btn btn-primary">الملف الاول</a>

                            </div>



                        </div>

                    }
                   

                    <br />
                    <br />
                    <br />




                 



                    
                 
                    @if (Model.OneUser.DocumentB != null)
                    {
                        <div class="row g-3">
                            <div class="col-md-12">


                                <a href="@string.Format("/Uploads/{0}",Model.OneUser.DocumentB)" class="btn btn-primary">الملف لثاني</a>

                            </div>



                        </div>

                    }

                  

                    <br />
                    <br />
                    <br />





                    @if (Model.OneUser.DocumentC != null)
                    {
                        <div class="row g-3">
                            <div class="col-md-12">


                                <a href="@string.Format("/Uploads/{0}",Model.OneUser.DocumentC)" class="btn btn-primary">الملف الثالث</a>

                            </div>



                        </div>

                    }


                 

                    <br />
                    <br />
                    <br />

                    @if (Model.OneUser.IdentityDocument != null) 
                    {
                        <div class="row g-3">
                            <div class="col-md-12">


                                <a href="@string.Format("/Uploads/{0}",Model.OneUser.IdentityDocument)" class="btn btn-primary">الهوية </a>

                            </div>



                        </div>
                    }
                 

                    <br />
                    <br />
                    <br />

                    <div class="row g-3">
                        <div class="col-md-12">

                            <label class="form-label" for="basic-default-fullname">تقييم المحامي عدد نجوم </label>
                            <input disabled asp-for="OneUser.EvaluationNoOfStatrs" type="text" class="form-control" id="basic-default-fullname" placeholder="  تقييم المحامي عدد نجوم" />

                        </div>



                    </div>



               



                    <div class="row g-3">
                        <div class="col-md-12">

                            <label class="form-label" for="basic-default-fullname">تقييم المحامي  نسبة</label>
                            <input disabled asp-for="OneUser.EvaluationNumerical" type="text" class="form-control" id="basic-default-fullname" placeholder=" تقييم المحامي  نسبة" />

                        </div>



                    </div>




                    <div class="row g-3">
                        <div class="col-md-12">

                            <label class="form-label" for="basic-default-fullname">الاسم الاول</label>
                            <input disabled asp-for="OneUser.FirstName" type="text" class="form-control" id="basic-default-fullname" placeholder="الاسم الاول" />

                        </div>



                    </div>


                  



                    <div class="row g-3">
                        <div class="col-md-12">

                            <label class="form-label" for="basic-default-fullname">  اسم العائلة</label>
                            <input disabled asp-for="OneUser.FamilyName" type="text" class="form-control" id="basic-default-fullname" placeholder="  اسم العائلة" />

                        </div>



                    </div>


                    <div class="row g-3">
                        <div class="col-md-12">

                            <label class="form-label" for="basic-default-fullname">   عدد ساعات العمل</label>
                            <input disabled asp-for="OneUser.HoursOfWork" type="text" class="form-control" id="basic-default-fullname" placeholder="عدد ساعات العمل" />

                        </div>



                    </div>


                    <div class="row g-3">
                        <div class="col-md-12">

                            <label class="form-label" for="basic-default-fullname">اسم القسم الرئيسي للاستشارة</label>
                            <input disabled asp-for="OneUser.MainConsultingName" type="text" class="form-control" id="basic-default-fullname" placeholder="اسم القسم الرئيسي للاستشارة" />

                        </div>



                    </div>



                    <div class="row g-3">
                        <div class="col-md-12">

                            <label class="form-label" for="basic-default-fullname">وصف مختصر للمحامي </label>
                            <input disabled asp-for="OneUser.ShortDescription" type="text" class="form-control" id="basic-default-fullname" placeholder=" وصف مختصر للمحامي" />

                        </div>



                    </div>



                    <div class="row g-3">
                        <div class="col-md-12">

                            <label class="form-label" for="basic-default-fullname">    حالة الاكونت </label>
                            <input disabled asp-for="OneUser.Status" type="text" class="form-control" id="basic-default-fullname" placeholder=" حالة الاكونت" />

                        </div>



                    </div>


                    <div class="row g-3">
                        <div class="col-md-12">

                            <label class="form-label" for="basic-default-fullname">   عدد سنوات الخبرة</label>
                            <input disabled asp-for="OneUser.YearsOfExperience" type="text" class="form-control" id="basic-default-fullname" placeholder="عدد سنوات الخبرة" />

                        </div>



                    </div>


                   






                  



                   




                   


                  


                   



                 




                  



                 


                   
 
                </form>
            </div>
        </div>

    </div>


</div>
@section Scripts{
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");

    }

 }