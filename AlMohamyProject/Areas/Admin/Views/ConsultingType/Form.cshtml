@model Domains.TbConsultingType
<div class="container-xxl flex-grow-1 container-p-y">
    <h4 class="py-3 breadcrumb-wrapper mb-4">
        <span class="text-muted fw-light">علوحة التحكم/</span>  انواع الاستشارة طلب محدد او غير محدد
    </h4>

    <!-- Basic Layout -->
    <div class="row">
        <div class="col-xl">



            <!-- Multi Column with Form Separator -->
            <div class="card mb-4">
                <h5 class="card-header">انواع الاستشارة</h5>
                <form enctype="multipart/form-data" method="post" role="form" asp-area="Admin" asp-controller="ConsultingType" asp-action="Save" class="card-body">


                    <div class="row g-3">
                        <div class="col-md-12">
                            <input type="hidden" asp-for="ConsultingTypeId">
                            <input type="hidden" asp-for="CreatedDate">
                            <label class="form-label" for="basic-default-fullname">عنوان  نوع الاستشارة</label>
                            <input asp-for="ConsultingTypeTitle" type="text" class="form-control" id="basic-default-fullname" placeholder="عنوان  نوع الاستشارة" />
                            <span asp-validation-for="ConsultingTypeTitle" class="text-danger"></span>
                        </div>
                        <div class="col-md-12">
                            <label class="form-label" for="basic-default-company">تفاصيل  نوع الاستشارة</label>
                            <input asp-for="ConsultingTypeDescription" type="text" class="form-control" id="basic-default-company" placeholder="تفاصيل  نوع الاستشارة" />
                            <span asp-validation-for="ConsultingTypeDescription" class="text-danger"></span>
                        </div>
                      

                    </div>
                    <div class="pt-4">
                        <button type="submit" class="btn btn-primary me-sm-3 me-1">Submit</button>

                    </div>
                </form>
            </div>
        </div>

    </div>


</div>
@section Scripts{
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");

    }

 }