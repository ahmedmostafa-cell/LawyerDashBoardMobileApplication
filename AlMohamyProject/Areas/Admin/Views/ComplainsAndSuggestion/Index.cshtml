@model AlMohamyProject.Models.HomePageModel
<div class="container-xxl flex-grow-1 container-p-y">
    <h4 class="py-3 breadcrumb-wrapper mb-4"><span class="text-muted fw-light">لوحة التحكم /</span>  الشكاوي و الاقتراحات</h4>

    <!-- DataTable with Buttons -->
    <div class="card">
        <div class="data_table">
            <table id="example" class="table table-striped table-bordered">
                <thead class="table-dark">
                    <tr>
                        <th>اسم صاحب الشكوي</th>
                        <th>ايميل صاحب الشكوي</th>
                        <th>التفاصيل</th>
                       



                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.lstComplainsAndSuggestions)
                    {

                        <tr>
                            <td>@item.Name</td>
                            <td>
                                @item.Email
                            </td>
                            <td>
                                <a href="/Admin/ComplainsAndSuggestion/Form2?id=@item.ComplaintsAndSuggestionsId" class='btn btn-info text-white' style='cursor:pointer'> تفاصيل</a>
                            </td>
                          




                        </tr>

                    }


                </tbody>
            </table>
        </div>
    </div>
  
</div>